decl
	int n, ar[100], i, j, x, temp;
    bool b;
enddecl
int main() {
begin
	write(1);
	write(2);
	read(n);
	write(3);
	i=0;
	while(i<n) do
		read(ar[i]);
		i=i+1;
	endwhile;
	write(111);
	i=0;
	j=1;
	while(i<n) do
		j=i+1;
		while(j<n) do
			if(ar[i]>ar[j]) then
				temp=ar[i];
				ar[i]=ar[j];
				ar[j]=temp;
			else
				temp=ar[i];
			endif;
			j=j+1;
		endwhile;
		i=i+1;
	endwhile;
	i=0;
	while(i<n) do
		write(ar[i]);
		i=i+1;
	endwhile;
	return 0;
end
}
