
decl
	integer i;
	boolean x;
enddecl

begin
	read(i);
	x = true;
	while (x == true) do
		write(i);
		if (i == 10) then
			i = i+1;
			write(111);
			x = false;
			continue;
			write(333);
		endif;
		if ( i == 15) then
			write(222);
			break;
			write(444);
		endif;
		write(i);
		i = i+1;
	endwhile;
	
	write(i+1);
end

